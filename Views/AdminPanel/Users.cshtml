@model IEnumerable<ArtistPortfolio.Models.Identity.ApplicationUser>
@{
    ViewData["Title"] = "Админ Панел";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}
<h2>Users</h2>

<table class="table table-bordered table-striped" >
    <thead>
        <tr>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
                <tr>
                    <td>@user.Email</td>
                    <td>@user.FirstName</td>
                    <td>@user.LastName</td>
                    <td>
                        <div class="w-75 btn-group" role="group">
                                <a asp-action="EditUser" asp-route-id="@user.Id" class="btn btn-primary mx-2">Edit</a>
                        </div>
                    </td>
                </tr>
        }
    </tbody>
</table>
